<?xml version="1.0"?>
<data>
    <xpath
        expr="/form/notebook/page[@id=&quot;general&quot;]"
            position="replace">
        <page string="General" id="general" col="6">
            <label name="name"/>
            <field name="name"/>
            <label name="typo"/>
            <field name="typo"/>
            <label name="code"/>
            <field name="code"/>
            <label name="moe"/>
            <field name="moe"/>
            <label name="moa"/>
            <field name="moa"/>
            <label name="chantier"/>
            <field name="chantier"/>
            <label name="suivi"/>
            <field name="suivi"/>
            <label name="active"/>
            <field name="active"/>
            <newline/>
            <label name="avancement"/>
            <field name="avancement"/>
            <label name="datedeb"/>
            <field name="datedeb"/>
            <label name="datefin"/>
            <field name="datefin"/>
            <notebook colspan="6">
                <page string="Site Map" col="2" id="map">
                    <field name="site_image" widget="image" img_width="1280" img_height="960" colspan="2"/>        
                    <button string="Edit geometries" name="site_edit" icon="tryton-executable" colspan="2"/>
                </page>
                <page string="Type" col="2" id="type">      
                    <field name="tracks" />
                    <field name="zones" />
                    <field name="points" />            
                </page>                
            </notebook>
        </page>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id=&quot;plot&quot;]"
            position="replace">
        <page string="Plot" col="1" id="plot">                        
            <field name="parcelle" colspan="6"/>
        </page>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id=&quot;situation_map&quot;]"
            position="replace">
        <page string="Situation Map" col="1" id="situation_map">                        
            <field name="site_situation" widget="image" img_width="1280" img_height="960" />
            <button string="Generate" name="site_situation_gen" icon="tryton-executable" />
        </page>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id=&quot;map&quot;]"
            position="replace">
        <page string="Image Map" col="1" id="map">                        
            <field name="site_map" widget="image" img_width="1280" img_height="960" />
            <button string="Generate" name="site_map_gen" icon="tryton-executable" />
        </page>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id=&quot;photo&quot;]"
            position="replace">
        <page string="Photo" col="1" id="photo">                        
            <field name="photo" widget="image" img_width="1280" img_height="960" />            
        </page>
    </xpath>
    <xpath
        expr="/form/notebook/page[@id=&quot;misc_obj&quot;]"
            position="replace">
        <page string="Misc. obj." id="misc_obj">
            <notebook colspan="1">
                <page string="Polygones" id="misc_obj_poly">
                    <field name="misc_obj_poly" />
                </page>
                <page string="Lines" id="misc_obj_line">
                    <field name="misc_obj_line" />
                </page>
                <page string="Points" id="misc_obj_point">
                    <field name="misc_obj_point" />
                </page>
            </notebook>
        </page>
    </xpath>                   
</data>

