<?xml version="1.0"?>
<data>
    <xpath
        expr="/form/field[@name=&quot;party&quot;]"
            position="after">
            <label name="pourcentage" />
            <field name="pourcentage" />            
    </xpath> 
    <xpath
        expr="/form/notebook/page[@id=&quot;shares&quot;]"
            position="after">
            <page string="Base forfaitaire agricole" id="bfa">
                <field name="bfa" />
            </page>
            <page string="Revenus fonciers" id="rf">
                <field name="rf" />
            </page>
            <page string="Plus-values" id="pv">
                <field name="pv" />
            </page>
    </xpath>                   
</data>

