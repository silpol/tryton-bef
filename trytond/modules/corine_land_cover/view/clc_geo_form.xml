        <page string="General" id="general">
            <label name="gid"/>
            <field name="gid"/>
            <label name="code"/>
            <field name="code"/>
            <label name="annee"/>
            <field name="annee"/>
            <label name="active"/>
            <field name="active"/>
        </page>                       
        <page string="Map" id="map" col="2">                            
            <field name="clc_image" colspan="1" widget="image" img_width="640" img_height="480"/>
            <field name="clc_map" colspan="1" widget="image" img_width="640" img_height="480"/>
            <button string="Edit" name="clc_edit" icon="tryton-executable" />
            <button string="Generate" name="generate" icon="tryton-executable" />
        </page>                        
    </notebook>
</form>
