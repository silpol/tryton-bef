<?xml version="1.0"?>
<form string="Diagnostic - Taxinomie mammifères">
    <notebook>
        <page string="Espèce" id="espece" col="4">
            <label name="taxinomie"/>
            <field name="taxinomie"/>
            <label name="liste"/>
            <field name="liste"/>
            <label name="prospection"/>
            <field name="prospection" widget="selection"/>
            <label name="effectif"/>
            <field name="effectif" widget="selection"/>
            <label name="indice"/>
            <field name="indice"/>
            <newline/>
            <field name="presence" colspan="4"/>
        </page>
        <page string="Photo" id="photo">
            <field name="photo" widget="image" img_width="800" img_height="600"/>                                                                
        </page>
    </notebook> 
</form>
