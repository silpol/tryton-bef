<?xml version="1.0"?>
<form string="pylone" col="2">
    <notebook colspan="2">
        <page string="General" id="general">
            <label name="equipement"/>
            <field name="equipement"/>            
            <label name="code"/>
            <field name="code"/>                            
            <label name="proprietaire"/>
            <field name="proprietaire" widget="selection"/>
            <label name="typo"/>
            <field name="typo"/>
            <label name="gestionnaire"/>
            <field name="gestionnaire" widget="selection"/>
            <label name="commune"/>
            <field name="commune"/>            
            <field name="portee" colspan="4"/>            
            <label name="active"/>
            <field name="active"/>                        
        </page>
        <page string="Travaux" id="travaux">
            <field name="travaux"/>
        </page>
        <page string="Map" id="map" col="2">
            <field name="image" colspan="1" widget="image" img_width="640" img_height="480"/>                                 
            <field name="image_map" colspan="1" widget="image" img_width="640" img_height="480"/>
            <button string="Edit" name="pylone_edit" icon="tryton-executable" />                           
            <button string="Generate" name="generate" icon="tryton-executable" />    
        </page>
    </notebook>
</form>
