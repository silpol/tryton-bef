<?xml version="1.0"?>
<form string="equipement" col="2">
    <notebook colspan="6">
        <page string="General" id="general">
            <label name="code"/>
            <field name="code"/>
            <label name="name"/>
            <field name="name"/>
            <label name="statut"/>
            <field name="statut" widget="selection"/>
            <label name="hierarchie"/>
            <field name="hierarchie" widget="selection"/>            
            <label name="active"/>
            <field name="active"/>
        </page>
        <page string="Portee" id="portee">            
            <field name="portee" />
        </page>
        <page string="Map" id="map" col="2">
            <field name="image" colspan="1" widget="image" img_width="640" img_height="480"/>                                 
            <field name="image_map" colspan="1" widget="image" img_width="640" img_height="480"/>
            <button string="Edit" name="equipement_edit" icon="tryton-executable" />                           
            <button string="Generate" name="generate" icon="tryton-executable" />    
        </page>
    </notebook>
</form>
