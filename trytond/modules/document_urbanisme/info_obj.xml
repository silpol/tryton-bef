        <field name="name">Lines</field>
        <field name="res_model">urba.info_obj_line</field>
    </record>

    <record model="ir.action.act_window.view" id="act_urba_info_obj_line_form_view1">
        <field name="sequence" eval="10"/>
        <field name="view" ref="urba_info_obj_line_view_tree"/>
        <field name="act_window" ref="act_urba_info_obj_line_form"/>
    </record>

    <record model="ir.action.act_window.view" id="act_urba_info_obj_line_form_view2">
        <field name="sequence" eval="20"/>
        <field name="view" ref="urba_info_obj_line_view_form"/>
        <field name="act_window" ref="act_urba_info_obj_line_form"/>
    </record>

    <menuitem parent="menu_urba_info_obj" sequence="1" id="menu_urba_info_obj_line_form" icon="tryton-list" action="act_urba_info_obj_line_form"/>

    <record model="ir.action.report" id="report_info_obj_line_edit">
        <field name="name">Open in QGis...</field>
        <field name="model">urba.info_obj_line</field>
        <field name="report_name">urba.info_obj_line.qgis</field>
        <field name="report">qgis/qgis.qgs</field>
    </record>
    <record model="ir.action.keyword" id="report_info_obj_line_edit_kwd">
        <field name="keyword">form_action</field>
        <field name="model">urba.info_obj_line,-1</field>
        <field name="action" ref="report_info_obj_line_edit"/>
    </record>

    <!-- urba info points objects -->
    <record model="ir.ui.view" id="urba_info_obj_point_view_tree">
        <field name="model">urba.info_obj_point</field>
        <field name="type">tree</field>
        <field name="name">point_tree</field>
    </record>

    <record model="ir.ui.view" id="urba_info_obj_point_view_form">
        <field name="model">urba.info_obj_point</field>
        <field name="type">form</field>
        <field name="name">point_form</field>
    </record>

    <record model="ir.action.act_window" id="act_urba_info_obj_point_form">
        <field name="name">Points</field>
        <field name="res_model">urba.info_obj_point</field>
    </record>

    <record model="ir.action.act_window.view" id="act_urba_info_obj_point_form_view1">
        <field name="sequence" eval="10"/>
        <field name="view" ref="urba_info_obj_point_view_tree"/>
        <field name="act_window" ref="act_urba_info_obj_point_form"/>
    </record>

    <record model="ir.action.act_window.view" id="act_urba_info_obj_point_form_view2">
        <field name="sequence" eval="20"/>
        <field name="view" ref="urba_info_obj_point_view_form"/>
        <field name="act_window" ref="act_urba_info_obj_point_form"/>
    </record>

    <menuitem parent="menu_urba_info_obj" sequence="1" id="menu_urba_info_obj_point_form" icon="tryton-list" action="act_urba_info_obj_point_form"/>

    <record model="ir.action.report" id="report_info_obj_point_edit">
        <field name="name">Open in QGis...</field>
        <field name="model">urba.info_obj_point</field>
        <field name="report_name">urba.info_obj_point.qgis</field>
        <field name="report">qgis/qgis.qgs</field>
    </record>
    <record model="ir.action.keyword" id="report_info_obj_point_edit_kwd">
        <field name="keyword">form_action</field>
        <field name="model">urba.info_obj_point,-1</field>
        <field name="action" ref="report_info_obj_point_edit"/>
    </record>
    </data>
</tryton>
