<tryton>
    <data>
    <menuitem parent="menu_urba_configuration" sequence="1" id="menu_urba_info_obj" name="Information objects" icon="tryton-list"/>

    <!-- information info polygone objects -->
    <record model="ir.ui.view" id="urba_info_obj_poly_view_tree">
        <field name="model">urba.info_obj_poly</field>
        <field name="type">tree</field>
        <field name="name">poly_tree</field>
    </record>

    <record model="ir.ui.view" id="urba_info_obj_poly_view_form">
        <field name="model">urba.info_obj_poly</field>
        <field name="type">form</field>
        <field name="name">poly_form</field>
    </record>

    <record model="ir.action.act_window" id="act_urba_info_obj_poly_form">
        <field name="name">Polygons</field>
        <field name="res_model">urba.info_obj_poly</field>
    </record>

    <record model="ir.action.act_window.view" id="act_urba_info_obj_poly_form_view1">
        <field name="sequence" eval="10"/>
        <field name="view" ref="urba_info_obj_poly_view_tree"/>
        <field name="act_window" ref="act_urba_info_obj_poly_form"/>
    </record>

    <record model="ir.action.act_window.view" id="act_urba_info_obj_poly_form_view2">
        <field name="sequence" eval="20"/>
        <field name="view" ref="urba_info_obj_poly_view_form"/>
        <field name="act_window" ref="act_urba_info_obj_poly_form"/>
    </record>

    <menuitem parent="menu_urba_info_obj" sequence="1" id="menu_urba_info_obj_poly_form" icon="tryton-list" action="act_urba_info_obj_poly_form"/>

    <record model="ir.action.report" id="report_info_obj_poly_edit">
        <field name="name">Open in QGis...</field>
        <field name="model">urba.info_obj_poly</field>
        <field name="report_name">urba.info_obj_poly.qgis</field>
        <field name="report">qgis/qgis.qgs</field>
    </record>
    <record model="ir.action.keyword" id="report_info_obj_poly_edit_kwd">
        <field name="keyword">form_action</field>
        <field name="model">urba.info_obj_poly,-1</field>
        <field name="action" ref="report_info_obj_poly_edit"/>
    </record>

    <!-- urba info lines objects -->
    <record model="ir.ui.view" id="urba_info_obj_line_view_tree">
        <field name="model">urba.info_obj_line</field>
        <field name="type">tree</field>
        <field name="name">line_tree</field>
    </record>

    <record model="ir.ui.view" id="urba_info_obj_line_view_form">
        <field name="model">urba.info_obj_line</field>
