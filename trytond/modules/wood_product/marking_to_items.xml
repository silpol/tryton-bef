<tryton>
    <data>
        <record model="ir.ui.view" id="wiz_item_to_mark_view">
            <field name="model">wood_product.marking_to_items.start</field>
            <field name="inherit" ref="wood_product.items_sheet_view_form" />
            <field name="arch" type="xml">
                <![CDATA[
                    <data>
                        <xpath
                            expr="/form/notebook/page[@id=&quot;wood_desc&quot;]"
                            position="replace">
                        </xpath>
                        <xpath
                            expr="/form/notebook/page/label[@name=&quot;main_variety&quot;]"
                            position="replace">
                        </xpath>
                        <xpath
                            expr="/form/notebook/page/field[@name=&quot;main_variety&quot;]"
                            position="replace">
                        </xpath>
                    </data>
                ]]>
            </field>
        </record>
    </data>
</tryton>
