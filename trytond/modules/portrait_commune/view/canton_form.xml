<?xml version="1.0"?>
<form string="Canton">
    <label name="region"/>
    <field name="region"/>
    <label name="departement"/>
    <field name="departement"/>
    <label name="commune"/>
    <field name="commune"/>
    <label name="nom"/>
    <field name="nom"/>
    <label name="code"/>
    <field name="code"/>
    <label name="version"/>
    <field name="version"/>
    <label name="active"/>
    <field name="active"/> 
    <notebook>
        <page string="Map" id="map" col="4">
            <field name="canton_map" widget="image" img_width="640" img_height="480" colspan="4"/>
            <newline/>    
            <button string="Generate" name="generate" icon="tryton-executable" colspan="4"/>
        </page>        
    </notebook>
</form>
