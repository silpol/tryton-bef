    <notebook colspan="4">
        <page string="General" id="general" col="2">
            <label name="validite"/>
            <field name="validite" widget="selection"/>
            <label name="substrat"/>
            <field name="substrat" widget="selection"/>
            <label name="superficie"/>
            <field name="superficie"/>
            <label name="classabond"/>
            <field name="classabond" widget="selection"/>
            <label name="classabondbota"/>
            <field name="classabondbota" widget="selection"/>
            <label name="recouvrementbota"/>
            <field name="recouvrementbota"/>
            <label name="nombre"/>
            <field name="nombre"/>
            <label name="presence"/>
            <field name="presence"/>
            <label name="typesexe"/>
            <field name="typesexe" widget="selection"/>
            <label name="typeffectif"/>
            <field name="typeffectif" widget="selection"/>
            <label name="comportement"/>
            <field name="comportement" widget="selection"/>
            <label name="etatsante"/>
