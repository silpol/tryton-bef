        <page string="Comment" col="1" id="comment">                        
            <field name="comment"/>           
        </page>
        <page string="Photo" col="1" id="photo">                        
            <field name="photo" widget="image" img_width="1280" img_height="960" />            
        </page>
        <page string="Map" col="2" id="map">            
            <field colspan="2" name="listetaxon_map" widget="image" img_width="600" img_height="600"/>
            <button string="Edit" name="pointlistetaxon_edit" icon="tryton-executable"/>
            <button string="Generate" name="generate" icon="tryton-executable"/>            
        </page>
    </notebook>    
</form>
