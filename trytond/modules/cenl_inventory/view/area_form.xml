<?xml version="1.0"?>
<form string="Zone de prospection" col="2">
    <notebook colspan="2">
        <page string="General" id="general" col="6">
            <label name="campagne"/>
            <field name="campagne"/>
            <label name="code"/>
            <field name="code"/>
            <label name="name"/>
            <field name="name"/>
            <label name="inventaire"/>
            <field name="inventaire"/>
            <label name="prospection"/>
            <field name="prospection"/>
            <label name="dispositif"/>
            <field name="dispositif"/>             
            <label name="suivi"/>
            <field name="suivi"/>            
            <label name="datedeb"/>
            <field name="datedeb"/>
            <label name="datefin"/>
            <field name="datefin"/>
            <label name="avancement"/>
            <field name="avancement"/>
            <label name="active"/>
            <field name="active"/>
            <field name="tracks" colspan="6"/>
            <field name="zones" colspan="6"/>
            <field name="points" colspan="6"/>
            <button colspan="4" string="Edit geometries" name="prospect_edit" icon="tryton-executable" />          
        </page>                          
        <page string="Area Map" col="1" id="prospect_map">                        
            <field name="prospect_situation" widget="image" img_width="1280" img_height="960" />
            <button string="Generate situation" name="prospect_situation_gen" icon="tryton-executable" />
        </page>
        <page string="Image Map" col="1" id="map">                        
            <field name="prospect_map" widget="image" img_width="1280" img_height="960" />
            <button string="Generate image" name="prospect_map_gen" icon="tryton-executable" />
        </page>
        <page string="Photo" col="1" id="photo">                        
            <field name="photo" widget="image" img_width="1280" img_height="960" />            
        </page>
        <page string="Objets divers" id="misc_obj">
            <notebook colspan="1">
                <page string="Polygones" id="misc_obj_poly">
                    <field name="misc_obj_poly" />
                </page>
                <page string="Lines" id="misc_obj_line">
                    <field name="misc_obj_line" />
                </page>
                <page string="Points" id="misc_obj_point">
                    <field name="misc_obj_point" />
                </page>
            </notebook>
        </page>
    </notebook>
</form>
