<?xml version="1.0"?>
<form string="Station" col="2">
    <group id="group_ug0" colspan="4" string="Unité de gestion">
        <label name="idug"/>
        <field name="idug"/>
        <label name="equipementug"/>
        <field name="equipementug"/>
        <label name="refug"/>
        <field name="refug"/>
        <label name="rueug"/>
        <field name="rueug"/>
        <label name="debug"/>
        <field name="debug"/>
        <label name="finug"/>
        <field name="finug"/>
        <label name="communeug"/>
        <field name="communeug" colspan="3"/>
    </group>
    <group id="group_station0" colspan="4" string="Station">
        <label name="id"/>
        <field name="id"/>
        <label name="code"/>
        <field name="code"/>
    </group>
    <notebook colspan="6">
        <page string="Général" id="general">
            <label name="ug"/>
            <field name="ug"/>                             
            <label name="proprietaire"/>
            <field name="proprietaire" widget="selection"/>
            <label name="domaine"/>
            <field name="domaine"/>
            <label name="gestionnaire"/>
            <field name="gestionnaire" widget="selection"/>                                                       
            <label name="distance"/>
            <field name="distance"/>
            <label name="commune"/>
            <field name="commune"/>
            <label name="active"/>
            <field name="active"/>                         
        </page>
        <page string="Gestion Emplacement" id="gestion_emplacement">
            <field name="emplacement" />
        </page>
        <page string="Map" id="map" col="1">                        
            <field name="station_map" colspan="1" widget="image" img_width="640" img_height="480"/>            
            <button string="Generate" name="generate" icon="tryton-executable" />
        </page>
    </notebook>
</form>
