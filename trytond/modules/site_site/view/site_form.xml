<?xml version="1.0"?>
<form string="Site" col="2">
    <notebook colspan="2">
        <page string="General" id="general" col="6">
            <label name="name"/>
            <field name="name"/>
            <label name="typo"/>
            <field name="typo"/>
            <label name="code"/>
            <field name="code"/>
            <label name="moe"/>
            <field name="moe"/>
            <label name="moa"/>
            <field name="moa"/>
            <label name="suivi"/>
            <field name="suivi"/>
            <label name="avancement"/>
            <field name="avancement"/>
            <label name="datedeb"/>
            <field name="datedeb"/>
            <label name="datefin"/>
            <field name="datefin"/>
            <label name="active"/>
            <field name="active"/>
            <field name="tracks" colspan="6"/>
            <field name="zones" colspan="6"/>
            <field name="points" colspan="6"/>         
            <button colspan="6" string="Edit geometries" name="site_edit" icon="tryton-executable" />
        </page>
        <page string="Plot" col="1" id="plot">                        
            <field name="parcelle" colspan="6"/>
        </page>                      
        <page string="Situation Map" col="1" id="situation_map">                        
            <field name="site_situation" widget="image" img_width="1280" img_height="960" />
            <button string="Generate" name="site_situation_gen" icon="tryton-executable" />
        </page>
        <page string="Image Map" col="1" id="map">                        
            <field name="site_image" widget="image" img_width="1280" img_height="960" />
            <button string="Generate" name="site_image_gen" icon="tryton-executable" />
        </page>
        <page string="Photo" col="1" id="photo">                        
            <field name="photo" widget="image" img_width="1280" img_height="960" />            
        </page>
        <page string="Misc. obj." id="misc_obj">
            <notebook colspan="1">
                <page string="Polygones" id="misc_obj_poly">
                    <field name="misc_obj_poly" />
                </page>
                <page string="Lines" id="misc_obj_line">
                    <field name="misc_obj_line" />
                </page>
                <page string="Points" id="misc_obj_point">
                    <field name="misc_obj_point" />
                </page>
            </notebook>
        </page>
    </notebook>
</form>
